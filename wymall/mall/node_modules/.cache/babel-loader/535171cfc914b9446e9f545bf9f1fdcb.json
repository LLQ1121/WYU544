AA,YAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iBAAA;;AAAA;AAAA;AAAA;AAAA;;AAhBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAsBA;AACA;AACA;AACA;AACA;AACA;AACA,EAAA,OAAA,EAAA;AACA,IAAA,OADA;AAAA;AAAA;AAAA,gDACA,EADA,EACA,IADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,GAAA,CAAA,SAAA,EAAA;AACA,kBAAA,MAAA,EAAA;AAAA,oBAAA,UAAA,EAAA,EAAA;AAAA,oBAAA,IAAA,EAAA;AAAA;AADA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;AAAA,kDAKA,GAAA,CAAA,IAAA,CAAA,IALA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AA5BA,CAAA","sourcesContent":["<template>\r\n  <div id=\"categoryList\">\r\n    <van-tabs v-model=\"active\">\r\n      <van-tab v-for=\"(item, index) in clist\" :key=\"index\" :title=\"item.name\">\r\n        <h3>{{ item.name }}</h3>\r\n        <p>{{ item.front_name }}</p>\r\n        <div v-if=\"item.plist\">\r\n          <van-grid :border=\"true\" :column-num=\"2\">\r\n            <van-grid-item\r\n              v-for=\"(item1, index1) in item.plist.data\"\r\n              :key=\"index1\"\r\n              :to=\"'/product/'+item1.id\"\r\n            >\r\n              <van-image\r\n                width=\"100\"\r\n                height=\"100\"\r\n                :src=\"item1.list_pic_url\"\r\n              />\r\n              <h4 class=\"van-ellipsis\">{{ item1.name }}</h4>\r\n              <p class=\"price\">${{item1.retail_price}}</p>\r\n            </van-grid-item>\r\n          </van-grid>\r\n        </div>\r\n      </van-tab>\r\n    </van-tabs>\r\n\r\n    <tab-button></tab-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport tabButton from \"../components/tabButton.vue\";\r\nimport axios from \"axios\";\r\nimport api from \"../assets/config/api.js\";\r\nexport default {\r\n  props: [\"id\"],\r\n  data() {\r\n    return {\r\n      active: \"\",\r\n      clist: [],\r\n    };\r\n  },\r\n  components: {\r\n    tabButton,\r\n  },\r\n  async created() {\r\n    let res = await axios.get(api.GoodsCategory, { params: { id: this.id } });\r\n    // console.log(res);\r\n    this.clist = res.data.data.brotherCategory;\r\n    let id = this.clist[0].id;\r\n    this.clist.forEach(async (item, index) => {\r\n      item.plist = await this.getlist(item.id, 1);\r\n      // console.log(item.plist.data)\r\n      this.$forceUpdate();\r\n    });\r\n  },\r\n  // watch: {\r\n  //   active: async function (index) {\r\n  //     let id = this.clist[index].id;\r\n  //     let page = 1;\r\n  //     let size = 20;\r\n  //   },\r\n  methods: {\r\n    async getlist(id, page) {\r\n      let res = await axios.get(api.GoodsList, {\r\n        params: { categoryId: id, page },\r\n      });\r\n      return res.data.data;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\">\r\n  #categoryList{\r\n    .van-ellipsis{\r\n      width: 100%;\r\n      font-size: 14px;\r\n      font-weight: 500;\r\n      padding: 0 10px;\r\n    }\r\n    .van-grid-item{\r\n      overflow: hidden;\r\n      box-sizing: border-box;\r\n    }\r\n    .price{\r\n      color: red;\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"src/views"}]}